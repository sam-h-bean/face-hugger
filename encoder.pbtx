name: "encoder"
platform: "python"
max_batch_size: 0

input {
        name: "TEXT"
        type: TYPE_STRING
        dims: [-1]
}

output [
    {
        name: "INPUT_IDS"
        type: TYPE_INT64
        dims: [-1, -1]
    },
    {
        name: "ATTENTION"
        type: TYPE_INT64
        dims: [-1, -1]
    }
]

instance_group [
    {
        count: 1
        kind: KIND_CPU
    }
]
